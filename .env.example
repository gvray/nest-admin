# NODE
NODE_ENV=development

# SERVER
PORT=3000

# DATABASE
DATABASE_URL="mysql://root:password@localhost:3306/app"

# JWT
JWT_SECRET=your-super-secret-key

# LOGGING
LOG_LEVEL=info

# CROS
ENABLE_CORS=
# CORS - 仅在开发环境生效 (生产环境请在 Nginx 配置)
# 多个域名用逗号分隔
CORS_ORIGIN="http://localhost:3000,http://localhost:3001,http://localhost:8080,http://localhost:9000"
